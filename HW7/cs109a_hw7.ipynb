    "# calculate false negative rate for each model\n",
    "f_neg = np.empty(len(models),dtype=float)\n",
    "\n",
    "# iterate over models\n",
    "for i,model in enumerate(models):\n",
    "    \n",
    "    # record mean cross-val accuracy\n",
    "    y_pred = model.predict(X_test)\n",
    "    prevalence = np.sum(y_test>1)/y_test.shape[0]\n",
    "    sensitivity = np.all((y_pred==1,y_test==1),axis=0).sum()/np.sum(y_test==1)\n",
    "    false_neg = (1-sensitivity)*prevalence\n",
    "    true_pos = sensitivity * prevalence\n",
    "    f_neg[i] = np.all((y_pred==1,y_test>1),axis=0).sum()/np.sum(y_test>1)\n",
    "\n",
    "f_neg_df = pd.DataFrame(data=f_neg.reshape(1,-1), columns=model_names)\n",
    "f_neg_df = f_neg_df.rename(index={0: \"false neg. rate\"})\n",
    "f_neg_df"
